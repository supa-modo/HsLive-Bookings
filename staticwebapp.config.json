{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/home",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/fleet",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/service/:id",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "/favicon.ico"]
  }
}
